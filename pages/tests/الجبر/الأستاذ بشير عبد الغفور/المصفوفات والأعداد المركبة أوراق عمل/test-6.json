[
  {
    "title": "<div class='eq-main long'><div class='long-1'><span class='text'>مصفوفة الأمثال للجملة التالية : </span></div><div class='long-2'><div class='eq'><span class='sign'>( I )</span><span class='eq-sign'>:</span><div class='continous'><div class='con-sign'></div><div class='con-eq'><div class='eq-row'>x + 2y = 5</div><div class='eq-row'>-2x + y = 3</div></div></div></div></div></div>",
    "name": "a-1",
